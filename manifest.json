{

    "manifest_version": 2,
    "name": "No Scroll Pretty Please",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "https://docs.google.com/presentation/*",
          "http://docs.google.com/presentation/*"
        ],
        "js": ["content.js"],
        "all_frames": true
      }
    ]
}
